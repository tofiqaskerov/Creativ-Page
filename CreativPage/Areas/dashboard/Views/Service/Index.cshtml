@model List<Service>

<a class="btn btn-success mb-4" href="/dashboard/service/Create">Create</a>
<table class="table table-bordered table-striped table-dark">
    <tr>
        <th>Id</th>
        <th>Title</th>
        <th>Icon</th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
            
            <td>
                <i class="@item.Icon fa-2x"></i>
            </td>
            <td>
                <a href="/dashboard/service/Edit/@item.Id" class="btn btn-outline-warning btn-sm ">Edit</a>
                <a href="" class="btn btn-outline-info btn-sm">Detail</a>
                <a href="/dashboard/service/Delete/@item.Id" class="btn btn-outline-danger btn-sm">Delete</a>
            </td>
        </tr>
    }


</table>